generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model BookSuggestion {
  id         Int    @id @default(autoincrement())
  booktitle  String
  bookAuthor String
  bookGenre  String
}

model Movie {
  id          Int    @id @default(autoincrement())
  movietitle  String
  movieAuthor String
  movieGenre  String
}